	<changeSet id="f15abc2c-ca8a-4fb7-bf55-7895491f4510" author="watchmen">
		<preConditions onFail="MARK_RAN">
			<not>
				<tableExists tableName="topic_rule_aggregate"/>
			</not>
        </preConditions>
        <createTable tableName="topic_rule_aggregate">
			<column name="id_" type="${pk-column.type}">
				<constraints primaryKey="true"/>
			</column>
			<column name="count" type="${number.type}"/>
			<column name="factorid" type="${text.type}"/>
			<column name="rulecode" type="${text.type}"/>
			<column name="topicid" type="${text.type}"/>
			<column name="aggregate_assist_" type="${aggregate-assist-column.type}"/>
			<column name="version_" type="${version-column.type}"/>
			<column name="tenant_id_" type="${tenant-column.type}"/>
			<column name="insert_time_" type="${audit-column.type}"/>
			<column name="update_time_" type="${audit-column.type}"/>
        </createTable>
	</changeSet>
	
	<!-- add or modify columns -->
	<!-- add count when column not exists -->
	<changeSet id="cc21cb32-fb13-46b6-b590-9210021d1486" author="watchmen">
		<preConditions onFail="MARK_RAN">
			<or>
				<and>
					<dbms type="mysql"/>
					<sqlCheck expectedResult="0">SELECT COUNT(1) FROM INFORMATION_SCHEMA.COLUMNS WHERE TABLE_SCHEMA = DATABASE() AND TABLE_NAME = 'topic_rule_aggregate' AND COLUMN_NAME = 'count'</sqlCheck>
				</and>
			</or>
			<or>
				<and>
					<dbms type="oracle"/>
					<sqlCheck expectedResult="0">SELECT COUNT(1) FROM USER_TAB_COLUMNS WHERE TABLE_NAME = 'topic_rule_aggregate' AND COLUMN_NAME = 'count'</sqlCheck>
				</and>
			</or>
       </preConditions>
        <addColumn tableName="topic_rule_aggregate">
        	<column name="count" type="${number.type}"/>
		</addColumn>
    </changeSet>

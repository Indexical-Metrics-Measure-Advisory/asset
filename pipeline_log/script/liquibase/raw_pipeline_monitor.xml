			<or>
				<and>
					<dbms type="mysql"/>
					<sqlCheck expectedResult="0">SELECT COUNT(1) FROM INFORMATION_SCHEMA.COLUMNS WHERE TABLE_SCHEMA = DATABASE() AND TABLE_NAME = 'topic_raw_pipeline_monitor' AND COLUMN_NAME = 'pipelineid'</sqlCheck>
				</and>
			</or>
			<or>
				<and>
					<dbms type="oracle"/>
					<sqlCheck expectedResult="0">SELECT COUNT(1) FROM USER_TAB_COLUMNS WHERE TABLE_NAME = 'topic_raw_pipeline_monitor' AND COLUMN_NAME = 'pipelineid'</sqlCheck>
				</and>
			</or>
       </preConditions>
        <addColumn tableName="topic_raw_pipeline_monitor">

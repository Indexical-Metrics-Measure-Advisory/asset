	<changeSet id="eb8d667f-3b6f-4e5f-811c-669d83b85b8a" author="watchmen">
		<preConditions onFail="MARK_RAN">
			<not>
				<tableExists tableName="topic_raw_pipeline_monitor"/>
			</not>
        </preConditions>
        <createTable tableName="topic_raw_pipeline_monitor">
			<column name="id_" type="${pk-column.type}">
				<constraints primaryKey="true"/>
			</column>
			<column name="pipelineid" type="${text.type}"/>
			<column name="status" type="${text.type}"/>
			<column name="topicid" type="${text.type}"/>
			<column name="traceid" type="${text.type}"/>
			<column name="data_" type="${json-column.type}"/>


			<column name="tenant_id_" type="${tenant-column.type}"/>
			<column name="insert_time_" type="${audit-column.type}"/>
			<column name="update_time_" type="${audit-column.type}"/>
        </createTable>
	</changeSet>
	
	<!-- add or modify columns -->
	<!-- add pipelineid when column not exists -->
